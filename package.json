{
  "name": "ml-image-processor-server",
  "version": "1.0.0",
  "description": "Images processing server based on AWS and NodeJS",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pznamir00/ML-image-processor-server.git"
  },
  "keywords": [
    "aws",
    "nodejs",
    "server"
  ],
  "author": "pznamir00",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pznamir00/ML-image-processor-server/issues"
  },
  "homepage": "https://github.com/pznamir00/ML-image-processor-server#readme",
  "dependencies": {
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "@types/sequelize": "^4.28.20",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.5.4"
  },
  "jest": {
    "preset": "ts-jest",
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
